{
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "jsonwebtoken": "8.3.0",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "23.3.9",
    "@types/jsonwebtoken": "8.3.0",
    "@types/morgan": "1.7.35",
    "@types/supertest": "2.0.6",
    "jest": "23.6.0",
    "nodemon": "1.18.5",
    "rimraf": "2.6.2",
    "supertest": "3.3.0",
    "ts-jest": "23.10.4",
    "typescript": "3.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.+(js|ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "name": "authentication",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "nodemon -x ts-node src/index.ts",
    "test": "jest --coverage --maxWorkers 2 --verbose"
  },
  "version": "1.0.0"
}
