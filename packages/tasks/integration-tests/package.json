{
  "devDependencies": {
    "@types/faker": "4.1.4",
    "@types/jest": "23.3.7",
    "@types/puppeteer": "1.9.0",
    "faker": "4.1.0",
    "jest": "23.6.0",
    "puppeteer": "1.9.0",
    "ts-jest": "23.10.4",
    "typescript": "3.1.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "**/*.test.+(js|ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "name": "integration-tests",
  "private": true,
  "scripts": {
    "start": "jest --verbose"
  },
  "version": "1.0.0"
}
