{
  "devDependencies": {
    "@types/faker": "4.1.4",
    "@types/jest": "23.3.10",
    "@types/puppeteer": "1.10.1",
    "apollo-boost": "0.1.22",
    "faker": "4.1.0",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "jest": "23.6.0",
    "node-fetch": "2.3.0",
    "puppeteer": "1.11.0",
    "supertest": "3.3.0",
    "ts-jest": "23.10.5",
    "typescript": "3.2.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "**/*.test.+(js|ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "name": "integration-tests",
  "private": true,
  "scripts": {
    "lint": "tslint --project . && tsc --noEmit",
    "start": "jest -ci --detectOpenHandles"
  },
  "version": "1.0.0"
}
