{
  "dependencies": {
    "@material-ui/core": "3.7.1",
    "apollo-cache-inmemory": "1.3.12",
    "apollo-client": "2.4.8",
    "apollo-link": "1.2.6",
    "apollo-link-http": "1.5.9",
    "autosuggest-highlight": "3.1.1",
    "classnames": "2.2.6",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "react-apollo": "2.3.3",
    "react-autosuggest": "9.4.2"
  },
  "devDependencies": {
    "apollo-storybook-react": "0.1.8",
    "@storybook/addon-actions": "4.1.3",
    "@storybook/addon-info": "4.1.3",
    "@storybook/react": "4.1.3",
    "@types/autosuggest-highlight": "3.1.0",
    "@types/classnames": "2.2.6",
    "@types/react-autosuggest": "9.3.6",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-info": "3.4.2",
    "@types/storybook__react": "4.0.0",
    "apollo-codegen": "0.20.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "jest": "23.6.0",
    "react": "16.7.0",
    "react-docgen-typescript-loader": "3.0.0",
    "rimraf": "2.6.2",
    "rollup": "0.68.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-postcss": "1.6.3",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-typescript2": "0.18.0",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.2",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/utilities/storybook/**",
      "!src/setupTests.ts",
      "!src/mocks/**"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/src/mocks/style.ts"
    },
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/*.test.+(js|ts|tsx)"
    ],
    "transform": {
      ".+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "name": "domain-model",
  "peerDependencies": {
    "material-design-icons-iconfont": "^4.0.2",
    "react": "^16.6.0"
  },
  "private": true,
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.ts",
    "download-schema": "scripts/download-schema.sh",
    "generate": "scripts/generate.sh",
    "lint": "tslint --project . && tsc --noEmit",
    "storybook": "start-storybook --port 3333",
    "test": "jest --coverage --maxWorkers 2 --verbose"
  },
  "version": "1.0.0"
}
