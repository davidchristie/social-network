{
  "dependencies": {
    "node-fetch": "2.2.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node-fetch": "2.1.2",
    "jest": "23.6.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.4",
    "typescript": "3.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.+(js|ts)"
    ],
    "transform": {
      ".+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "main": "dist/index.js",
  "name": "public-api-context",
  "peerDependencies": {
    "express": "^4.16.4",
    "graphql-yoga": "^1.16.7"
  },
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "tslint --project . && tsc --noEmit",
    "test": "jest --coverage --maxWorkers 2 --verbose"
  },
  "version": "1.0.0"
}
