{
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/puppeteer": "^1.6.3",
    "jest": "^23.5.0",
    "puppeteer": "^1.7.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/*.test.+(js|ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "name": "end-to-end",
  "private": true,
  "scripts": {
    "test": "jest --verbose"
  },
  "version": "1.0.0"
}
