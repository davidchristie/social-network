{
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": false,
    "no-console": false,
    "no-empty-interface": false,
    "no-shadowed-variable": false,
    "semicolon": [
      true,
      "always"
    ]
  }
}
